<?xml version="1.0" encoding="ISO-8859-1"?>
<!--   
    Document   : Production inventory override file
    Created on : Dec 12 2013
    Author     : Jerrol Krause
    Description:
        The purpose of this file is for temporary hotfixes to inventory levels that need to happen quicker than the 4-6 hour alfresco release. Changes in this file will override production values.
        THIS IS FOR URGENT HOTFIXES ONLY AND SHOULD NOT BE FOR REGULAR USE OR LONG TERM CHANGES. 
-->
<payload>
    <phones>
        <!-- 
        #### Phone Options ####
        @id - IMM SKU of phone
        @oos-override - Force the phone out of stock. Set to true to force OOS.
        @oos-threshold - Custom threshold to set to make a phone go automatically out of stock. Can be set to negative to force a phone IN STOCK. Needs to be a number.
        @default-id - Must go on the master variation ID. Allows you to override the default sku for phones with variations
        @map - override the default map value
        
        ## Code Sample ##
        <phone id="SPHL710ABB" oos-override="true" oos-threshold="3000" default-id="885909950546" map="610" />
        -->

        <phone id="885909741670" default-id="885909962549"/>
        <phone id="885909727711"  oos-threshold="50"/>
        <phone id="885909741700"  oos-threshold="60"/>
        <phone id="885909741717"  oos-threshold="200"/>
        <phone id="885909741687"  oos-threshold="40"/>
        <phone id="SPHG92032ABB"  oos-threshold="30"/>
        <phone id="HTC0PCV1ABB"  oos-threshold="100" map="48"/>
